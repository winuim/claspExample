{
  "name": "claspexample",
  "version": "1.0.0",
  "description": "",
  "main": "hello.js",
  "scripts": {
    "push": "npm run build && npm run deploy",
    "build": "rollup --config --bundleConfigAsCjs",
    "deploy": "clasp push",
    "pretest": "npm run lint",
    "test": "jest",
    "posttest": "npm run compile",
    "lint": "gts lint",
    "fix": "gts fix",
    "compile": "tsc --pretty --noEmitOnError",
    "clean": "gts clean && rm -rf ./coverage ./dist",
    "test-coverage": "jest --silent=true --coverage",
    "test-only": "jest --silent=true -o",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.@(ts|tsx)": [
      "npm run lint",
      "jest --bail --findRelatedTests --passWithNoTests",
      "npm run fix"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/winuim/claspExample.git"
  },
  "author": "Uema Yohei<winuim@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/winuim/claspExample/issues"
  },
  "homepage": "https://github.com/winuim/claspExample#readme",
  "dependencies": {
    "@types/google-apps-script": "^1.0.57",
    "@types/google-apps-script-oauth2": "^38.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/plugin-proposal-decorators": "^7.20.13",
    "@babel/plugin-transform-runtime": "^7.19.6",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@google/clasp": "^2.4.2",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^24.0.1",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@types/jest": "^29.2.5",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "gts": "^3.1.1",
    "husky": "^8.0.3",
    "jest": "^29.3.1",
    "lint-staged": "^13.1.0",
    "rollup": "^3.10.0",
    "rollup-plugin-copy": "^3.4.0",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  }
}
